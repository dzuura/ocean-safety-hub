{
  "name": "pelaut-hebat-backend",
  "version": "1.0.0",
  "description": "Backend API untuk Pelaut Hebat - Platform Keselamatan Transportasi Laut dan Kesejahteraan Masyarakat Pesisir",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "keywords": [
    "ocean",
    "safety",
    "marine",
    "weather",
    "api",
    "express"
  ],
  "author": "Pelaut Hebat Team",
  "license": "MIT",
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/__tests__/**"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.js"
    ]
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "axios": "^1.10.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "express": "^4.21.2",
    "express-rate-limit": "^8.0.1",
    "firebase": "^12.0.0",
    "firebase-admin": "^13.4.0",
    "helmet": "^8.1.0",
    "morgan": "^1.10.1",
    "node-cron": "^4.2.1"
  },
  "devDependencies": {
    "jest": "^30.0.4",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.3"
  }
}
